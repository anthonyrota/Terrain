{
    "name": "terrain",
    "version": "0.0.1",
    "license": "MIT",
    "scripts": {
        "start": "parcel serve ./src/index.html",
        "build": "rimraf public .parcel-cache && parcel build ./src/index.html --dist-dir public --no-cache",
        "lint": "eslint . && prettier --list-different . && cspell '**/*/{ts,js,md}' && tsc --noEmit"
    },
    "dependencies": {
        "gl-matrix": "3.3.0"
    },
    "devDependencies": {
        "@types/node": "14.14.31",
        "@typescript-eslint/eslint-plugin": "4.15.2",
        "@typescript-eslint/parser": "4.15.2",
        "cspell": "5.3.3",
        "eslint": "7.20.0",
        "eslint-config-prettier": "8.1.0",
        "eslint-import-resolver-typescript": "2.4.0",
        "eslint-plugin-import": "2.22.1",
        "parcel": "2.0.0-beta.1",
        "parcel-reporter-static-files-copy": "1.2.2",
        "prettier": "2.2.1",
        "rimraf": "3.0.2",
        "typescript": "4.2.2"
    }
}
